    <bpmn:scriptTask id="CalculateRiskFactors" name="Calculate risk factors" scriptFormat="JavaScript">
      <bpmn:incoming>Flow_0dl9ivv</bpmn:incoming>
      <bpmn:outgoing>Flow_0xjncuy</bpmn:outgoing>
      <bpmn:script>function calculateAge(date) 
{ 
    var diff_ms = Date.now() - date.getTime();
    var age_dt = new Date(diff_ms); 
  
    return Math.abs(age_dt.getUTCFullYear() - 1970);
}



// calculate Age Risk Factor
var bithDate = execution.getVariable("birthDate");
var age = calculateAge(bithDate); 
execution.setVariable("age", age);
var riskFactor = 0;
if(age &gt; 45)
{
  riskFactor = 20;
}
if(age &gt; 55)
{
  riskFactor = 50;
}
if(age &gt; 60)
{
  riskFactor = 70;
}
if(age &gt; 70)
{
  riskFactor = 90;
}
execution.setVariable("ageRiskFactor", riskFactor);



// calculate General Conditions Risk Factor
var height = execution.getVariable("height");
var weight = execution.getVariable("weight");
var bmi = (100 * 100 *  weight) / (height * height);
riskFactor = 0;
if(bmi &lt; 20)
{
  riskFactor = 50;
}
if(bmi &gt; 29)
{
  riskFactor = 50;
}
if(bmi &gt; 32)
{
  riskFactor = 70;
}
if(bmi &gt; 35)
{
  riskFactor = 100;
}
execution.setVariable("generalConditionsRiskFactor", riskFactor);



// calculate Health History Risk Factor
var riskCategory = execution.getVariable("riskCategory");
riskFactor = 0;
if(riskCategory == 'C1')
{
  riskFactor = 20;
}
if(riskCategory == 'C2')
{
  riskFactor = 50;
}
if(riskCategory == 'C3')
{
  riskFactor = 100;
}
execution.setVariable("healthHistoryRiskFactor", riskFactor);</bpmn:script>
    </bpmn:scriptTask>
