    <bpmn:scriptTask id="RateCalculation" name="Calculate monthly insurance rate" scriptFormat="JavaScript">
      <bpmn:incoming>Flow_1pqsw4x</bpmn:incoming>
      <bpmn:incoming>Flow_03nnhd7</bpmn:incoming>
      <bpmn:incoming>Flow_0vrg2ts</bpmn:incoming>
      <bpmn:outgoing>Flow_10r5mw0</bpmn:outgoing>
      <bpmn:script>var age = execution.getVariable("age");
var riskSurcharge = execution.getVariable("riskSurcharge");
if(!riskSurcharge)
{
riskSurcharge = 0;
}
var isExistingCustomer = execution.getVariable("isExistingCustomer");

var rate = Math.max(110, 10 * age) + riskSurcharge;

if(isExistingCustomer == true)
{
  rate = rate - 5;
}

execution.setVariable("rate", rate);</bpmn:script>
    </bpmn:scriptTask>
