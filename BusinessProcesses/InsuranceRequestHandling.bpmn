      <bpmn:script>var ageRF = execution.getVariable("ageRiskFactor");
var gcRF = execution.getVariable("generalConditionsRiskFactor");
var hhRF = execution.getVariable("healthHistoryRiskFactor");

var d = null;

if(d == null &amp;&amp; gcRF == 100)
{
  d = "NotEligibleForInsurance";
}
if(d == null &amp;&amp; hhRF == 100)
{
  d = "NotEligibleForInsurance";
}
if(d == null &amp;&amp; ageRF &gt;= 70)
{
  d = "ManualCheck";
}
if(d == null &amp;&amp; gcRF &gt;= 70)
{
  d = "ManualCheck";
}
if(d == null &amp;&amp; hhRF &gt;= 50)
{
  d = "ManualCheck";
}
if(d == null &amp;&amp; ageRF &gt;= 50 &amp;&amp; gcRF &gt;= 50)
{
  d = "ManualCheck";
}
if(d == null &amp;&amp; ageRF &gt;= 50 &amp;&amp; gcRF &lt; 50)
{
  d = "EligibleForInsurance";
}
if(d == null &amp;&amp; ageRF &lt; 50 &amp;&amp; gcRF &gt;= 50)
{
  d = "EligibleForInsurance";
}
if(d == null &amp;&amp; ageRF &lt; 50 &amp;&amp; gcRF &lt; 50 &amp;&amp; hhRF &lt; 50)
{
  d = "EligibleForInsurance";
}
execution.setVariable("premiumTariffDecision" ,d);</bpmn:script>
